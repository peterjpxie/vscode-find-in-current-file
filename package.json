{
    "name": "find-in-current-file",
    "displayName": "Find in Current File",
    "description": "Search only in the current file",
    "version": "0.0.8",
    "license": "MIT",
    "publisher": "peterjpxie",
    "engines": {
        "vscode": "^1.70.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/peterjpxie/vscode-find-in-current-file"
    },
    "homepage": "https://github.com/peterjpxie/vscode-find-in-current-file",
    "categories": [
        "Other"
    ],
    "icon": "images/icon.png",
    "activationEvents": [
        "onCommand:find-in-current-file.searchInCurrentFile"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "find-in-current-file.searchInCurrentFile",
                "title": "Find in Current File"
            }
        ],
        "keybindings": [
            {
                "command": "find-in-current-file.searchInCurrentFile",
                "key": "ctrl+alt+c",
                "mac": "cmd+alt+c",
                "when": "editorFocus || editorIsOpen"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.46.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^10.2.0",
        "typescript": "^3.9.10",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "vsce": "^1.97.0"
    }
}